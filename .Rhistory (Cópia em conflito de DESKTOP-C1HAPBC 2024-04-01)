blogdown:::serve_site()
#Define função e plot de titulação
fa = function(pH,pKa) {
x=0
for(i in 1:length(pKa)) {
x = x+1/(1 + 10^(pH - pKa[i]))}
return(x)
}
pKa=c(2.2,7.2,12.7)
curve(fa(x,pKa),1,14, xlab="pH", ylab="fa",
col=2)
#Define função e plot de titulação
fa = function(pH,pKa) {
x=0
for(i in 1:length(pKa)) {
x = x+1/(1 + 10^(pH - pKa[i]))}
return(x)
}
pKa=c(2.2,3.9,9.5)
curve(fa(x,pKa),1,14, xlab="pH", ylab="fa",
col=2)
